{
  "name": "bot-coder",
  "displayName": "A wise coding-bot, can auto write code for you, accept inline suggestions",
  "description": "auto generate && auto accept",
  "version": "1.1.0",
  "publisher": "HaoFuYang",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:coder-view",
    "onCommand:coding.start",
    "onCommand:coding.stop"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "coder-view",
          "title": "coderView",
          "icon": "media/icon.png"
        }
      ]
    },
    "views": {
      "coder-view": [
        {
          "type": "webview",
          "id": "coder-view",
          "name": "Coder View"
        }
      ]
    },
    "commands": [
      {
        "command": "panelDetails.show",
        "title": "Show Panel Details"
      },
      {
        "command": "coding.start",
        "title": "Start Coding"
      },
      {
        "command": "coding.stop",
        "title": "Stop Coding"
      },
      {
        "command": "autoInlineGenerator.tab",
        "title": "Tab Auto Inline Generator"
      }
    ]
  }
}
