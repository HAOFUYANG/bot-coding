{
  "name": "bot-coder",
  "displayName": "A wise coding-bot, can auto write code for you, accept inline suggestions",
  "description": "自动生成&自动采纳",
  "version": "1.0.0",
  "publisher": "HaoFuYang",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:coding.start",
    "onCommand:coding.stop"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "coding.start",
        "title": "Start Coding"
      },
      {
        "command": "coding.stop",
        "title": "Stop Coding"
      },
      {
        "command": "autoInlineGenerator.tab",
        "title": "Tab Auto Inline Generator"
      }
    ]
  },
  "scripts": {
    "package": "vsce package"
  },
  "devDependencies": {
    "vsce": "^2.15.0"
  }
}
